{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "offset": [ 0, 0, 0 ],
  //     "surroundingMargin": 0,
  //     "surroundingBlocks": "default"
  // }
  "spawn": null,
  
  "spawns": {
    "small island": {
      "north": [],
      "east": [],
      "south": [[ 2, 4, 3 ]],
      "west": []
    }
  },
  
  // A list of blocks which can be used to surround islands/caves.
  "surroundingBlocks": {
    "default": ["minecraft:water"]
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "Small Island",
      "file": "small_island.nbt",
      "spawns": "small island"
    }
  ]
}
